<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wufanfirstkotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/blue_left"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/left"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirstKotlin">
        <activity android:name=".yufalianxi.DatePickerActivity"></activity>
        <activity android:name=".yufalianxi.CheckBoxActivity" />
        <activity android:name=".yufalianxi.DialogActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".yufalianxi.WebViewActivity" />
    </application>

</manifest>